  "bugs": {
    "url": "https://github.com/khach2010/Miishop-E-commerce-webshop/issues"
  },
  "homepage": "https://github.com/khach2010/Miishop-E-commerce-webshop#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.15"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
